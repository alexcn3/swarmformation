<launch>

	<include file="$(find stdr_robot)/launch/robot_manager.launch"/>
	 
	<node type="stdr_server_node"
	      pkg="stdr_server"
	      name="stdr_server"
	      outputs="screen"
	      args="$(find stdr_resources)/maps/very_simple_room.yaml"/>
	      
	<node pkg="tf"
	      type="static_transform_publisher"
	      name="world2map"
	      args="0 0 0 0 0 0  world map 100"/>
	      
	<include file="$(find stdr_gui)/launch/stdr_gui.launch"/>
	
	<node pkg="stdr_robot"
	      type="robot_handler"
	      name="$(anon robot_spawn)"
	      args="add $(find swarm_formation_py)/robot/squarerobot.yaml 1 2 0"/>
	      
</launch>
